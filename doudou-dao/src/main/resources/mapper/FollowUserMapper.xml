	<resultMap id="BaseResultMap" type="com.doudou.dao.entity.FollowUser">
		<id column="id" property="id" />
		<result column="tx_id" property="txId" />
		<result column="client_id" property="clientId" />
		<result column="follow_client_id" property="followClientId" />
		<result column="last_message" property="lastMessage" />
		<result column="created" property="created" />
		<result column="modified" property="modified" />
		<result column="flag" property="flag" />
	</resultMap>

    <!-- 通用查询结果列-->
    <sql id="Base_Column_List">
        id, tx_id AS txId, client_id AS clientId, follow_client_id AS followClientId, last_message AS lastMessage, created, modified, flag
